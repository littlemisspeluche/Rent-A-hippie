
<h1>Jobs#index</h1>
<p>Find me in app/views/jobs/index.html.erb</p>


<h1>This is the Index jobs page </h1>
<div class="container">
<table class="table table-hover">
  <tr>
    <th>Description</th>
    <th>Location</th>
    <th>Cost</th>
    <th>Time</th>
    <th>User</th>
  </tr>
<% @job.each do |job| %>
  <tr>
    <td><%=link_to  job.description, job_path(job.id)%></td>
    <td><%=job.location%></td>
    <td><%=job.cost%></td>
    <td><%=job.time%></td>
    <td><%=job.user.first_name%></td>
  </tr>
<% end  %>
</table>
</div>
<%= link_to "Sign Out", destroy_user_session_path, :method =>:delete %>
<%= link_to "Add Job", new_job_path %>







